<ul class="nav nav-pills justify-content-center my-3">
{{#each (getRoutes)}}
  <li class="nav-item">
    {{#if (eq this ../page)}}
    <a class="nav-link active" aria-current="page" href="#">{{this}}</a>
    {{else}}
    <a class="nav-link" href="{{lower this}}">{{this}}</a>
    {{/if}}
  </li>
{{/each}}
</ul>